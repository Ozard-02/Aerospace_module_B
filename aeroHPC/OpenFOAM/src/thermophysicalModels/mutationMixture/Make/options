# Suppress warnings
C++WARN += -Wno-unused-function \
           -Wno-int-in-bool-context \
           -Wno-ignored-qualifiers \
           -Wno-sign-compare \
           -Wno-misleading-indentation \
           -Wno-unused-variable \
           -Wno-deprecated-copy

