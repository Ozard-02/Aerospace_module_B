/*---------------------------------------------------------------------------*\
References:
    1) Park et al., JTHT 15(1):76-90, 2001.
    2) Park, JTHT 7(3):385-398, 1993.

Air 11-species: e- N+ O+ NO+ N2+ O2+ N O NO N2 O2
\*---------------------------------------------------------------------------*/

reactions
{
    /*---------------------------------------------------------------------------*\
      DISSOCIATION REACTIONS
    \*---------------------------------------------------------------------------*/

    reaction-1 // Park 2001, O2+ Park 1993
    {
        type            reversibleThirdBodyArrhenius;
        reaction        "N2 = 2N";
        A               7.0e+18; // [m3 kmol-1 s-1] = 7.0e+21 [cm3 mol-1 s-1]
        beta            -1.6;
        Ta              113200;
        coeffs          11 
        (
            // (C 4.285714286)  // 3.0e+22 [cm3 mol-1 s-1]
            (O 4.285714286)     // 3.0e+22 [cm3 mol-1 s-1]
            (N 4.285714286)     // 3.0e+22 [cm3 mol-1 s-1]
            // (H 4.285714286)  // 3.0e+22 [cm3 mol-1 s-1]
            // (CO 1)
            // (C2 1)
            (N2 1)
            // (CN 1)
            (NO 1)
            // (CH 1)
            // (OH 1)
            (O2 1)
            // (H2 1)
            // (C3 1)
            // (C2H 1)
            // (C+ 1)
            // (H+ 1)
            (O+ 1)              // ! In Park 1993: same as O
            (N+ 1)              // ! In Park 1993: same as N
            (NO+ 1)
            (N2+ 1)
            (O2+ 1)
            (e- 0)              // Considered in reaction-e below
            //(e- 428.5714286)  // 3.0e+24 [cm3 mol-1 s-1] 
            // In Park 1993: 1.2e+25 [cm3 mol-1 s-1] 
        )
        ;
        isDissociative	yes;
    }

    reaction-2 // Park 2001
    {
        type            reversibleArrhenius;
        reaction        "N2 + e- = 2N + e-";
        A               3.0e+21; // [m3 kmol-1 s-1] = 3.0e+24 [cm3 mol-1 s-1]
        beta            -1.6;
        Ta              113200;
        isDissociative	yes;
    }


    reaction-3 // Park 2001, O2+ from park 1993
    {
        type            reversibleThirdBodyArrhenius;
        reaction        "O2 = 2O";
        A               2.0e+18;  // [m3 kmol-1 s-1] = 2.0e+21 [cm3 mol-1 s-1]
        beta            -1.5;
        Ta              59360;
        coeffs          11 
        (
            // (C 5)            // 1.0e+22 [cm3 mol-1 s-1]
            (O 5)               // 1.0e+22 [cm3 mol-1 s-1]
            (N 5)               // 1.0e+22 [cm3 mol-1 s-1]    
            // (H 5)            // 1.0e+22 [cm3 mol-1 s-1]
            // (CO 1)
            // (C2 1)
            (N2 1)
            // (CN 1)
            (NO 1)
            // (CH 1)
            // (OH 1)
            (O2 1)
            // (H2 1)
            // (C3 1)
            // (C2H 1)
            // (C+ 1)
            // (H+ 1)
            (O+ 1)              // ! In Park 1993: same as O
            (N+ 1)              // ! In Park 1993: same as N
            (NO+ 1)
            (N2+ 1)
            (O2+ 1)
            (e- 1) 
        )
        ;
        isDissociative	yes;
    }

    reaction-4 // Park 1993
    {
        type            reversibleThirdBodyArrhenius;
        reaction        "NO = N + O";  
        A               5.0e+12;  // [m3 kmol-1 s-1] = 5.0e+15 [cm3 mol-1 s-1]
        beta            0.0;
        Ta              75500;
        coeffs          11 
        (
            (O 22)              // 1.1e+17 [cm3 mol-1 s-1]
            (N 22)              // 1.1e+17 [cm3 mol-1 s-1]
            (N2 1)      
            (NO 22)             // 1.1e+17 [cm3 mol-1 s-1]
            (O2 1)
            (O+ 22)             // 1.1e+17 [cm3 mol-1 s-1]
            (N+ 22)             // 1.1e+17 [cm3 mol-1 s-1]
            (NO+ 1)
            (N2+ 1)
            (O2+ 1)
            (e- 1) 
        )
        ;
        isDissociative	yes;
    }

    /*---------------------------------------------------------------------------*\
      NO EXCHANGE REACTIONS
      \*---------------------------------------------------------------------------*/
    reaction-5 // Park 2001
    {
        type            reversibleArrhenius;
        reaction        "NO + O = N + O2";
        A               8.4e+9;  // [m3 kmol-1 s-1] = 8.4e+12 [cm3 mol-1 s-1]
        beta            0.0;
        Ta              19400;
