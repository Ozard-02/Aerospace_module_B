FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      physicalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

thermoType
{
    type            highEnthalpyThermo;
    mixture         multicomponentMixture;
    transport       sutherland;
    thermo          janaf;
    energy          sensibleEnthalpy;
    equationOfState perfectGas;
    specie          specie;
}

// Mutation++ mixture name for two-temperature model
mixture         air_5;

// Relaxation time step for energy transfer
relaxationTimeStep 1e-8;

defaultSpecie N2;

#include "speciesThermo.janaf"

