# GSL (needed by canteraToFoam)
export GSL_VERSION=2.7

export GSL_ARCH_PATH=$POLIMI_THIRD_PARTY_DIR/platforms/$WM_ARCH/gsl-$GSL_VERSION

_foamAddLib $GSL_ARCH_PATH/lib

